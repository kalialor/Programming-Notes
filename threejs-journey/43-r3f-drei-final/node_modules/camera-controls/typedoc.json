{
	"includeVersion": true,
	"markedOptions": {
		"breaks": true
	},
	"entryPoints": ["./src/CameraControls.ts"],
	"out": "docs",
	"excludePrivate": true,
	"excludeProtected": true,
	"excludeInternal": true,
	"excludeExternals": true,
