App.js

function App() {
    const age = 20;
    const isGreen = true;

    return(
        <div>
            {age >= 18 ? <h1>Over Age</h1> : <h1> Under Age</h1>}
            <h1 style={{ color: isGreen ? "green" : "red" }}> TEXT</h1>
        </div>
);
}
export default App;




